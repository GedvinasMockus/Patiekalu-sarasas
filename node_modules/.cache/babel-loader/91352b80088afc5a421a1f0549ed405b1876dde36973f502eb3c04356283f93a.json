ue)\r\n        const empty = ref(false)\r\n        const deleteModal = ref(false)\r\n        const createModal = ref(false)\r\n\r\n        const claims = computed(() => {\r\n            if(props.token)\r\n                return jwtDecode(props.token)\r\n        })\r\n\r\n        const fetchRestaurants = () => {\r\n            axios.get(`${process.env.VUE_APP_API}/restaurant`)\r\n                .then((res) => {\r\n                    restaurants.value = res.data\r\n                    loading.value = false\r\n                }).catch((err) => {\r\n                    if(err.response?.status === 401){\r\n                        axios.post(`${process.env.VUE_APP_API}/refresh`)\r\n                            .then((res) => {\r\n                                const accessToken = res.data.data.token\r\n                                context.emit('refresh', accessToken)\r\n                                fetchRestaurants()\r\n                            }).catch((err) => {\r\n\r\n                            })\r\n                    }\r\n                    else if(err.response?.status === 404)\r\n                    {\r\n                        empty.value = true\r\n                        loading.value = false\r\n                    }\r\n                    if(!err.response) {\r\n                        toast.error('Server is currently offline', {\r\n                            position: POSITION.TOP_CENTER,\r\n                            timeout: 3000,\r\n                            closeButton: CloseButton\r\n                        })\r\n                    }\r\n                    else console.log(err)\r\n                })\r\n        }\r\n\r\n        const deleteRestaurant = (index) => {\r\n            deleteModal.value = true;\r\n        }\r\n\r\n        const deleteModalClose = () => {\r\n            deleteModal.value = false;\r\n        }\r\n\r\n        const createRestaurant = () => {\r\n            createModal.value = true;\r\n        }\r\n\r\n        const createModalClose = () => {\r\n            createModal.value = false;\r\n        }\r\n        \r\n        onMounted(() => {\r\n            fetchRestaurants() // Initial fetch of restaurants\r\n        })\r\n\r\n        return { restaurants, loading, empty, claims, deleteRestaurant, deleteModalClose, createRestaurant, createModalClose, deleteModal, createModal }\r\n\r\n        return { \r\n            restaurants\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style>\r\n.blue-lighten-5--text {\r\n  background-color: var(--v-blue-lighten-5);\r\n}\r\n</style>"],"mappings":"AAiFA,SAASA,QAAQ,EAAEC,QAAQ,EAAEC,QAAQ,EAAEC,GAAG,EAAEC,SAAQ,QAAS,KAAI;AACjE,OAAOC,KAAI,MAAO,OAAM;AACxB,OAAOC,aAAY,MAAO,iCAAgC;AAC1D,OAAOC,WAAU,MAAO,+BAA8B;AACtD,OAAOC,WAAU,MAAO,+BAA8B;AACtD,SAASC,SAAQ,QAAS,YAAW;AACrC,SAASC,QAAQ,EAAEC,QAAO,QAAS,oBAAmB;AACtD,OAAOC,MAAK,MAAO,UAAS;AAC5B,eAAe;EACXC,KAAK,EAAE,CAAC,QAAQ,EAAE,WAAW,EAAE,OAAO,CAAC;EACvCC,UAAU,EAAE;IACRP,WAAW;IACXD;EACJ,CAAC;EACDS,KAAKA,CAACF,KAAK,EAAEG,OAAO,EAAC;IAEjB,MAAMC,KAAI,GAAIN,QAAQ,CAAC;IACvB,MAAMO,WAAU,GAAIf,GAAG,CAAC,EAAE;IAC1B,MAAMgB,OAAM,GAAIhB,GAAG,CAAC,IAAI;IACxB,MAAMiB,KAAI,GAAIjB,GAAG,CAAC,KAAK;IACvB,MAAMkB,WAAU,GAAIlB,GAAG,CAAC,KAAK;IAC7B,MAAMmB,WAAU,GAAInB,GAAG,CAAC,KAAK;IAE7B,MAAMoB,MAAK,GAAIvB,QAAQ,CAAC,MAAM;MAC1B,IAAGa,KAAK,CAACW,KAAK,EACV,OAAOf,SAAS,CAACI,KAAK,CAACW,KAAK;IACpC,CAAC;IAED,MAAMC,gBAAe,GAAIA,CAAA,KAAM;MAC3BpB,KAAK,CAACqB,GAAG,CAAE,GAAEC,OAAO,CAACC,GAAG,CAACC,WAAY,aAAY,EAC5CC,IAAI,CAAEC,GAAG,IAAK;QACXb,WAAW,CAACc,KAAI,GAAID,GAAG,CAACE,IAAG;QAC3Bd,OAAO,CAACa,KAAI,GAAI,KAAI;MACxB,CAAC,CAAC,CAACE,KAAK,CAAEC,GAAG,IAAK;QACd,IAAGA,GAAG,CAACC,QAAQ,EAAEC,MAAK,KAAM,GAAG,EAAC;UAC5BhC,KAAK,CAACiC,IAAI,CAAE,GAAEX,OAAO,CAACC,GAAG,CAACC,WAAY,UAAS,EAC1CC,IAAI,CAAEC,GAAG,IAAK;YACX,MAAMQ,WAAU,GAAIR,GAAG,CAACE,IAAI,CAACA,IAAI,CAACT,KAAI;YACtCR,OAAO,CAACwB,IAAI,CAAC,SAAS,EAAED,WAAW;YACnCd,gBAAgB,CAAC;UACrB,CAAC,CAAC,CAACS,KAAK,CAAEC,GAAG,IAAK,CAElB,CAAC;QACT,OACK,IAAGA,GAAG,CAACC,QAAQ,EAAEC,MAAK,KAAM,GAAG,EACpC;UACIjB,KAAK,CAACY